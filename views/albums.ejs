<div class="container">
  <div class="row">
    <% for(i=0; i<albums.length; i++){ %>
      <div class="col-lg-4 artist-container">
        <% if(albums[i].images[0]){%>
          <img src="<%=albums[i].images[0].url%>" alt="" class="artist-img">
        <%} else {%>
          <img src="http://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg" alt="" class="artist-img">
        <%}%>
        <div class="artist-info-container">
          <p><%=albums[i].name%></p>
          <a href="/tracks/<%=albums[i].id%>">View Tracks</a>
        </div>
      </div>
      <%}%>
  </div>
</div>
